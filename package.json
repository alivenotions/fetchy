  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.8",
    "@types/node-fetch": "2.5.7",
    "create-test-server": "3.0.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "node-fetch": "2.6.1",
    "prettier": "2.1.2",
    "semantic-release": "17.2.3",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
